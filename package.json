{
	"name": "ionic-boilerplate",
	"version": "0.0.1",
	"author": "Edwin Palacios",
	"license": "MIT",
	"homepage": "https://edwinpalacios.github.io",
	"description": "Boilerplate Ionic 3",
	"scripts": {
		"clean": "ionic-app-scripts clean",
		"lint": "ionic-app-scripts lint",
		"build": "ionic-app-scripts build",
		"ionic:build:after": "./scripts/build.sh",
		"ionic:build": "ionic-app-scripts build",
		"ionic:serve": "ionic-app-scripts serve",
		"doc:build": "compodoc -p tsconfig.json --hideGenerator --disableCoverage",
		"doc:serve": "compodoc -p tsconfig.json -s -r 3030 --hideGenerator --disableCoverage",
		"docs:serve:watch": "compodoc -p tsconfig.json -s -r 3030 -w --hideGenerator --disableCoverage"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/EdwinPalacios/ionic-boilerplate.git"
	},
	"dependencies": {
		"@angular/common": "5.2.4",
		"@angular/compiler": "5.2.4",
		"@angular/compiler-cli": "5.2.4",
		"@angular/core": "5.2.4",
		"@angular/forms": "5.2.4",
		"@angular/http": "5.2.4",
		"@angular/platform-browser": "5.2.4",
		"@angular/platform-browser-dynamic": "5.2.4",
		"@ionic-native/core": "4.5.3",
		"@ionic-native/splash-screen": "4.5.3",
		"@ionic-native/status-bar": "4.5.3",
		"@ionic/storage": "^2.1.3",
		"@ngx-translate/core": "^9.0.2",
		"@ngx-translate/http-loader": "^2.0.1",
		"cordova-android": "7.0.0",
		"cordova-plugin-device": "^2.0.1",
		"cordova-plugin-ionic-webview": "^1.1.16",
		"cordova-plugin-splashscreen": "^5.0.1",
		"cordova-plugin-whitelist": "^1.3.3",
		"cordova-sqlite-storage": "^2.2.0",
		"ionic-angular": "3.9.2",
		"ionic-plugin-keyboard": "^2.2.1",
		"ionicons": "3.0.0",
		"purify-css": "^1.2.6",
		"rxjs": "5.5.6",
		"sw-toolbox": "3.6.0",
		"zone.js": "0.8.20"
	},
	"devDependencies": {
		"@compodoc/compodoc": "^1.0.5",
		"@ionic/app-scripts": "3.1.8",
		"typescript": "2.7.1"
	},
	"config": {
		"ionic_purge_unused_fonts": false,
		"ionic_sass": "./config/config_sass.js",
		"ionic_copy": "./config/ionic_copy.js",
		"ionic_webpack": "./config/webpack.config.js"
	},
	"cordova": {
		"plugins": {
			"ionic-plugin-keyboard": {},
			"cordova-plugin-whitelist": {},
			"cordova-plugin-device": {},
			"cordova-plugin-splashscreen": {},
			"cordova-plugin-ionic-webview": {},
			"cordova-sqlite-storage": {}
		},
		"platforms": [
			"android"
		]
	}
}
